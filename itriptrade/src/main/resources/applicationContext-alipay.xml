<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.kgc.itrip.trade.config.AlipayConfig">
		<property name="signType" value="RSA2"/>
		<property name="returnUrl" value="http:localhost:8080/trade/api/return"/>
		<property name="notifyUrl" value="http:localhost:8080/trade/api/notify"/>
		<property name="charset" value="UTF-8"/>
		<property name="url" value="https://openapi.alipaydev.com/gateway.do"/>
		<property name="alipayPublicKey"
				  value=""/>
		<property name="rsaPrivateKey"
				  value=""/>
		<property name="format" value="json"/>
		<property name="appID" value="2016091700532201"/>
		<property name="paymentSuccessUrl" value="http://localhost:8080/index.html#/orderpaystate?orderNo=%s&amp;id=%s"/>
		<property name="paymentFailureUrl" value="http://localhost:8080/index.html#/orderpaystate?orderNo=%s&amp;id=%s&amp;state=0"/>
	</bean>
</beans>